{
  "ConnectionStrings": {
    "Postgres": ""
  },
  "Database": {
    "AutoMigrate": true
  },
  "Admin": {
    "ApiKey": ""
  },
  "DrawSync": {
    "UserAgent": "ProbabilitesLotoEuroMillions/1.0 (+https://localhost)",
    "HttpTimeoutSeconds": 30,
    "Loto": {
      "HistoryUrl": "https://www.fdj.fr/jeux-de-tirage/loto/historique",
      "RuleStartDate": "2019-11-04"
    },
    "EuroMillions": {
      "HistoryUrl": "https://www.fdj.fr/jeux-de-tirage/euromillions-my-million/historique",
      "RuleStartDate": "2016-09-01"
    }
  },
  "Subscriptions": {
    "PublicBaseUrl": "http://localhost:8080",
    "TokenSecret": "dev-only-change-me"
  },
  "Smtp": {
    "Host": "localhost",
    "Port": 1025,
    "UseStartTls": false,
    "Username": "",
    "Password": "",
    "SenderName": "Probabilites Loto & EuroMillions",
    "SenderAddress": "no-reply@example.local"
  },
  "RateLimiting": {
    "SubscriptionsPost": {
      "PermitLimit": 10,
      "WindowSeconds": 60
    }
  },
  "Cors": {
    "AllowedOrigins": []
  },
  "HealthChecks": {
    "Smtp": {
      "Enabled": false
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
